<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<!-- Emoticons from gnome-icon-theme -->

<!-- It is unclear what the text should be for tired, embarrassed and sick, so they were not used. -->

<emoticons imagefile="simple-gnome.png" >

<!-- crying - must be before sad -->
<emoticon left="24" top="48" right="48" bottom="72">
  <emotext>:'(</emotext>
  <emotext>:,(</emotext>
  <emotext>:((</emotext>
</emoticon>

<!-- angry - must be before sad -->
<emoticon left="72" top="48" right="96" bottom="72">
  <emotext>&#38;gt;:O</emotext>
  <emotext>XO</emotext>
  <emotext>:-(o)</emotext>
</emoticon>

<!-- monkey - another one that must be before sad -->
<emoticon left="96" top="48" right="120" bottom="72">
  <emotext>:-(|)</emotext>
  <emotext>:(|)</emotext>
  <emotext>8-|)</emotext>
</emoticon>

<!-- smile-big - must be before smile -->
<emoticon left="96" top="0" right="120" bottom="24">
  <emotext>:-D</emotext>
  <emotext>:D</emotext>
  <emotext>:-}</emotext>
  <emotext>:-))</emotext>
  <emotext>:))</emotext>
</emoticon>

<!-- smile -->
<emoticon left="0" top="0" right="24" bottom="24">
  <emotext>:-)</emotext>
  <emotext>:)</emotext>
</emoticon>

<!-- plain -->
<emoticon left="0" top="24" right="24" bottom="48">
  <emotext>:-|</emotext>
  <emotext>:|</emotext>
</emoticon>

<!-- sad -->
<emoticon left="0" top="48" right="24" bottom="72">
  <emotext>:-(</emotext>
  <emotext>:(</emotext>
</emoticon>

<!-- wink -->
<emoticon left="24" top="0" right="48" bottom="24">
  <emotext>;-)</emotext>
  <emotext>;)</emotext>
</emoticon>

<!-- surprise -->
<emoticon left="24" top="24" right="48" bottom="48">
  <emotext>:-O</emotext>
  <emotext>:O</emotext>
  <emotext>:-o</emotext>
  <emotext>=-O</emotext>
  <emotext>=-o</emotext>
</emoticon>

<!-- smirk -->
<emoticon left="48" top="0" right="72" bottom="24">
  <emotext>;^)</emotext>
</emoticon>

<!-- worried -->
<emoticon left="48" top="24" right="72" bottom="48">
  <emotext>:-\</emotext>
  <emotext>:\</emotext>
</emoticon>

<!-- uncertain (maybe the gnome icons worried/uncertain are the wrong way around, worried looks like ":-S") -->
<emoticon left="48" top="48" right="72" bottom="72">
  <emotext>:-S</emotext>
  <emotext>:-s</emotext>
  <emotext>:S</emotext>
  <emotext>:s</emotext>
</emoticon>

<!-- laugh -->
<emoticon left="72" top="0" right="96" bottom="24">
  <emotext>XD</emotext>
  <emotext>xD</emotext>
</emoticon>

<!-- raspberry (sticking out tongue) - strangely absent from the simple-tango theme... -->
<emoticon left="72" top="24" right="96" bottom="48">
  <emotext>:-P</emotext>
  <emotext>:-p</emotext>
  <emotext>:-b</emotext>
  <emotext>:-Þ</emotext>
  <emotext>:P</emotext>
  <emotext>:p</emotext>
  <emotext>:b</emotext>
  <emotext>:Þ</emotext>
</emoticon>

<!-- kiss -->
<emoticon left="96" top="24" right="120" bottom="48">
  <emotext>:-*</emotext>
  <emotext>:*</emotext>
</emoticon>

<!-- cool (sunglasses) -->
<emoticon left="120" top="0" right="144" bottom="24">
  <emotext>8-)</emotext>
  <emotext>8)</emotext>
</emoticon>

<!-- angel -->
<emoticon left="120" top="24" right="144" bottom="48">
  <emotext>O:-)</emotext>
  <emotext>o:-)</emotext>
  <emotext>O:)</emotext>
  <emotext>o:)</emotext>
</emoticon>

<!-- devilish -->
<emoticon left="120" top="48" right="144" bottom="72">
  <emotext>]:-&#38;gt;</emotext>
  <emotext>&#38;gt;:)</emotext>
  <emotext>}:)</emotext>
</emoticon>

</emoticons>
